{
    "include": [
        "/etc/xdg/waybar/config"
    ],
    "modules-left": ["sway/workspaces"],
    "modules-center": ["sway/mode"],
    "position": "bottom",
    "height": 20,
    "custom/weather": {
        "icon-size": 42,
        "format": "{}",
        "tooltip": true,
        "interval": 3600,
        "exec": "/usr/share/sway/scripts/weather.py 'reykjavik'",
        "return-type": "json"
    }
}
